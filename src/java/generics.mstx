import java.duplicates
import java.types.references

maybe-type-parameters-ok(s_cls, maybeTypeParameters) :- maybeTypeParameters match
  { None()                     -> true
  | Some(TypeParameters(ftps)) -> false
  }.

maybe-type-arguments-ok(s_cls, maybeTypeParameters) :- maybeTypeParameters match
  { None()                     -> true
  | Some(TypeParameters(ftps)) -> false
  }.

